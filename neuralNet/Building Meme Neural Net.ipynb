   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "```\n",
    "textgen.reset()\n",
    "textgen.train_from_file(\n",
    "    file_path=file_name,\n",
    "    num_epochs=20,\n",
    "    batch_size=1024,\n",
    "    dropout=0.2,\n",
    "    #rnn_bidirectional=True,\n",
    "    validation=True,\n",
    "    rnn_layers=2,\n",
    "    rnn_size=256,)\n",
